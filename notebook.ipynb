    "from plotnine import ggplot, aes, geom_point\n",
    "import pandas as pd\n",
    "\n",
    "# example \n",
    "data = pd.DataFrame({\n",
    "\n",
    "    'x': [1,2,3,4,5],\n",
    "    'y': [5,4,3,2,1]\n",
    "})\n",
    "\n",
    "#scatter plot\n",
    "plot = ggplot(data, aes(x='x',y='y')) + geom_point()\n",
    "\n",
    "print(plot)"
   ]
  }
